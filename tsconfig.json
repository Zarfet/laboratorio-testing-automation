{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2018",
    "module": "commonjs",
    "lib": ["dom", "es6"],
    "allowJs": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "types": ["cypress", "node"]
  },
  "include": [
    "cypress/**/*",
    "cypress.config.js"
  ],
  "exclude": [
    "node_modules",
    "cypress/videos",
    "cypress/screenshots",
    "cypress/reports",
    "cypress/results"
  ]
}